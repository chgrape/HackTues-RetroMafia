{
    "manifest_version": 3,
    "name": "Retro-mafia",
    "description": "Ad statistics",
    "version": "1.0",
    "host_permissions": ["<all_urls>"],
    "icons":{
        "16": "/icons/16.png",
        "48": "/icons/48.png",
        "128": "/icons/128.png"
    },
    "content_scripts":[
        {
            "matches":[
                "<all_urls>"
            ],
            "js": [
                "content-script.js"
            ]
        }
    ]
}